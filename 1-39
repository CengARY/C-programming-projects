#include <stdio.h>
int main(){
    int age60;
    int ageLess10;
    int luggageMore20;
    int age = 0;
    int luggageWeight = 0;
    
    scanf("%d %d", &age, &luggageWeight);
    
    age60 = age == 60;
    ageLess10 = age < 10;
    luggageMore20 = luggageWeight > 20;
    
    if(age60){
        printf("0");
    }else{
        if(ageLess10){
            printf("5");
        }else{
            if(luggageMore20){
                printf("40");
                
            }else{
                printf("30");
            }
        }
    }
}
